#include <iostream>

using namespace std;

int main()
{
    int array[3][4];
    for (int i=0; i<3; i++)
    for (int j=0; j<4; j++)
    array[i][j]=rand() % 100;
    
    for (int i=0; i<3; i++) {
        cout << endl;
        for (int j=0; j<4; j++)
        cout << " " << array[i][j];
    }
    
    int min;
    
    for (int i=0; i<3; i++) {
        min = 100;
        for (int j=0; j<4; j++)
            if (min>array[i][j]) {
            min == array[i][j];
            array[i][j] == 1;
            }
    }
    
    
    cout << endl;
    for (int i=0; i<3; i++) {
        cout << endl;
        for (int j=0; j<4; j++)
        cout << " " << array[i][j];
    }
    
    cout << endl;
    int array[s];
    for (int i=0; i<3; i++){
        for (int j=0; j<4; j++) {
            array[s] += array[s] * array[i][j];
            cout << " " << array[s];
        }
        array[s]=0;
    }
    

    return 0;
}
